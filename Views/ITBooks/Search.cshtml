@model IEnumerable<Trabajo_Grupal.DTO.ITBooksDTO>
    
@if (Model == null || !Model.Any())
{
    <p>No se encontraron libros.</p>
}
else
{
    <table>
        <tr>
            <th>title</th>
            <th>subtitle</th>
            <th>isbn13</th>
            <th>price</th>
            <th>ImageName</th>
            <th>url</th>
        </tr>
        @foreach (var book in Model)
        {
            <tr>
                <td>@book.title</td>
                <td>@book.subtitle</td>
                <td>@book.isbn13</td>
                <td>@book.price</td>
                <td>@book.ImageName</td>
                <td>@book.url</td>
            </tr>
        }
    </table>
}