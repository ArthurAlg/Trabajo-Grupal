@model Trabajo_Grupal.DTO.SearchResultDTO

@if (Model != null && Model.Books != null){
    <table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Subtitle</th>
            <th>ISBN13</th>
            <th>Price</th>
            <th>Image</th>
            <th>URL</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var book in Model.Books)
        {
            <tr>
                <td>@book.title</td>
                <td>@book.subtitle</td>
                <td>@book.isbn13</td>
                <td>@book.price</td>
                <td>@book.ImageName</td>
                <td>@book.url</td>
            </tr>
        }
    </tbody>
</table>
}else{
    <p>No hay datos para mostrar.</p>
}
